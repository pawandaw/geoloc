<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>My Simple GeoApp</name>

  <description>
      A simple mobile application which focuses on geolocation and maps.
  </description>

  <author href="https://build.phonegap.com" email="dawokheep884@gmail.com.com">
      Dawokhee Pawan
  </author>

    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-geolocation" version="2.1.0" />
    <access origin="*"/>
	
    <plugin name="cordova-plugin-whitelist" version="1" />
	
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
	
</widget>